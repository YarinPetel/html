    $(function () {
        try{
            $('img').each(function () {
                if($(this).attr("alt") == undefined){
                    $(this).attr("alt",'');
                }
            });
        }catch (e) {}
        try {
            $(".lazy").lazyload({
                threshold: 50
            });
            if ( $(document).width() > 1100 ) {
                // load css start
                var h   = document.getElementsByTagName('head')[0];
                var s   = document.createElement('link');
                s.type  = 'text/css';
                s.rel  = 'stylesheet';
                s.href = "https://static.lovepik.com/css/lovepik/active/2022bd.v1.0.css";
                s.async = true;
                h.appendChild(s);
                // load css end
                function addBG(imgObj,dom,callback) {
                    if(imgObj.width > 0){
                        callback(dom,imgObj.src);
                    }else{
                        setTimeout(function () {
                            addBG(imgObj,dom,callback);
                        },10);
                    }
                }

                function loadcss(src, fn) {
                    var node = src;
                    if (node.attachEvent) {
                        //IE
                        node.attachEvent('onload', function () {
                            fn(null, node)
                        });
                    } else {
                        //other browser
                        setTimeout(function () {
                            poll(node, fn);
                        }, 0);
                    }
                    function poll(node, callback) {
                        var isLoaded = false;
                        if (/webkit/i.test(navigator.userAgent)) {
                            //webkit
                            if (node['sheet']) {
                                isLoaded = true;
                            }
                        } else if (node['sheet']) {
                            // for Firefox
                            try {
                                if (node['sheet'].cssRules) {
                                    isLoaded = true;
                                }
                            } catch (ex) {
                                // NS_ERROR_DOM_SECURITY_ERR
                                if (ex.code === 1000) {
                                    isLoaded = true;
                                }
                            }
                        }
                        if (isLoaded) {
                            setTimeout(function () {
                                callback(null, node);
                            }, 1);
                        } else {
                            setTimeout(function () {
                                poll(node, callback);
                            }, 10);
                        }
                    }
                    node.onLoad = function () {
                        fn(null, node);
                    }
                }
                //判断css加载完成与否
                loadcss(s,function (e) {
                    $(".anniversary_head_two").show();
