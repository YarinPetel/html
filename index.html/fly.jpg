                        env.setCookie('lp_bc_pao','',1);
                    },
                    error:function(err){
                    console.log(err);
                }

                });
            }
        }
    }
    document.onmouseover = function () {
        setBc();
    };
    //移动端兼容
    document.addEventListener('touchmove',function () {
        setBc();
    },false);

</script>


